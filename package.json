{
  "name": "rxjs-fs",
  "version": "2.0.0",
  "description": "Functional asynchronous file system operations with RxJSv5",
  "main": "lib/FileSystem.js",
  "jsnext:main": "src/FileSystem.js",
  "scripts": {
    "test": "jest",
    "test-coverage": "jest --coverage",
    "debug": "node --debug-brk --inspect ./node_modules/.bin/jest -i --env jest-environment-node-debug",
    "clean": "rimraf lib && mkdirp lib",
    "prepublish": "npm test && npm run build",
    "build": "npm run clean && babel src -d lib --source-maps",
    "watch": "npm run clean && babel src -d lib --source-maps --watch"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:trxcllnt/rxjs-fs.git"
  },
  "dependencies": {
    "rxjs": "^5.0.3",
    "symbol-observable": "^1.0.4"
  },
  "files": [
    "lib",
    "src",
    "README.md"
  ],
  "keywords": [
    "rx",
    "rxjs",
    "fs"
  ],
  "author": "Paul Taylor <paul.e.taylor@me.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/trxcllnt/rxjs-fs/issues"
  },
  "jest": {
    "verbose": true,
    "testPathDirs": [
      "src/"
    ]
  },
  "devDependencies": {
    "babel-cli": "~6.22.2",
    "babel-jest": "~18.0.0",
    "babel-plugin-transform-runtime": "~6.22.0",
    "babel-polyfill": "~6.22.0",
    "babel-preset-es2015": "~6.22.0",
    "babel-preset-stage-0": "~6.22.0",
    "cross-env": "~3.1.4",
    "jest": "~18.1.0",
    "jest-environment-node-debug": "0.0.2",
    "mkdirp": "~0.5.1",
    "rimraf": "~2.5.4"
  }
}
